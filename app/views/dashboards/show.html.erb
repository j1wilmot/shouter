<%= form_for @text_shout do |form|  %>
  <%= label :public, "private" %>
  <%= check_box_tag :public, "1", false %>
  <%= form.text_field :body, placeholder: "Shout Here" %>
  <%= form.submit "Shout" %>
<% end %>

<%= form_for(@photo_shout) do |form|  %>
  <%= label :public, "private" %>
  <%= check_box_tag :public, "1", false %>
  <%= form.file_field :image %>
  <%= form.submit "Shout" %>
<% end %>

<%= render @timeline %>
